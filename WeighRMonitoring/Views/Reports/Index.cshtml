@model IEnumerable<WeighRMonitoring.Models.ReportCategory>
@using System.Web.UI.WebControls
@using WeighRMonitoring.Models;
@{
    ViewBag.Title = "Reports";
}
<link href="~/Content/jsTree/themes/default/style.min.css" rel="stylesheet" />
<div id="title-breadcrumb-option-demo" class="page-title-breadcrumb">
        <select id="reportsPara" class="col-sm-6 col-md-2 form-control para-select">
            <option value="">Select Report</option>
            <option value="">Production - General Report</option>
            <option value="">Production - Detailed Report</option>
            <option value="">Production - Product Per Machine Summary Report</option>
            <option value="">Production - Product Per Machine Detailed Report</option>
            <option value="">Production - Daily Detailed Report</option>
            <option value="">Production - Daily Summary Report</option>
            <option value="">Production - Weekly Detailed Report</option>
            <option value="">Production - Weekly Summary Report</option>
            <option value="">Production - Monthly Detailed Report</option>
            <option value="">Production - Monthly Summary Report</option>

            <option value="">Machine - Detailed Report</option>
            <option value="">Machine - Machine Per Product Detailed Report</option>
            <option value="">Machine - Machine Per Product Summary Report</option>
            <option value="">Machine - Fill-Time Per Machine Summary Report</option>
            <option value="">Machine - Precision Per Machine Summary Report</option>

            <option value="">Batch - Products Per Batch Detailed Report</option>
            <option value="">Batch - Products Per Batch Summary Report</option>
            <option value="">Batch - Batches Per Product Detailed Report</option>
            <option value="">Batch - Batches Per Product Summary Report</option>

        </select>
        <select id="machinesPara" class="col-sm-6 col-md-2 form-control para-select">
            <option value="">Select Machine</option>
        </select>
        <select id="productsPara" class="col-sm-6 col-md-2 form-control para-select">
            <option value="">Select Product</option>
        </select>
        <input type="text" id="dtpDateFrom" class="col-sm-6 col-md-2 form-control" placeholder="Date From" />
        <input type="text" id="dtpDateTo" class="col-sm-6 col-md-2 form-control" placeholder="Date To" />
        <input type="button" id="btnView" class="col-sm-6 col-md-2 btn btn-primary" value="View" />
    <div class="clearfix">
    </div>
</div>

<!--BEGIN CONTENT-->
<div class="page-content">
    <div id="tab-general">
        <div id="sum_box" class="row mbl">

            <div class="col-md-8 weigh-page-container">
                <iframe id="iframeReportViewer" style="height:560px;width:99%" frameborder="0" scrolling="no"></iframe>
                @section scripts{
                    <script>

                url = "../Reports/ReportViewerPanel.aspx";
                var rframe = document.getElementById("iframeReportViewer");
                if (rframe !== null) {
                    if (rframe.src) {
                        rframe.src = url;
                    } else if (rframe.contentWindow !== null && rframe.contentWindow.location !== null) {
                        rframe.contentWindow.location = url;
                    } else { rframe.setAttribute('src', url); }
                }

                    </script>
                }

            </div>

        </div>

    </div>

</div>
<!--END CONTENT-->
<script src="~/Scripts/jquery-1.10.2.min.js"></script>

   


